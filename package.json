    "express-session": "^1.18.1",
    "cors": "^2.8.5",
    "discord-oauth2": "^2.12.0",
    "axios": "^1.7.9",
    "discord-html-transcripts": "^3.2.0",
    "@napi-rs/canvas": "^0.1.58",
    "colors": "^1.4.0",
    "ms": "^2.1.3",
    "uuid": "^11.0.5"
